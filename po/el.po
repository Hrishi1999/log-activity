msgid "Previous"
msgstr "Προηγούμενο"

#: logviewer.py:392
msgid "Next"
msgstr "Επόμενο"

#: logviewer.py:401
msgid "Edit"
msgstr "Επεξεργασία"

#: logviewer.py:406
msgid "Delete Log File"
msgstr "Διαγραφή αρχείου καταγραφής"

#: logviewer.py:425
msgid "Tools"
msgstr "Εργαλεία"

#: logviewer.py:478
msgid "Error"
msgstr "Σφάλμα"

