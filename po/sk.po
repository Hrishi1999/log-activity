msgstr "Chyba: Súbor '%(path)s' neexistuje."

#: logviewer.py:206
#, python-format
msgid "ERROR: Unable to read file '%(file)s'."
msgstr "Chyba: Nemôžem čítať súbor '%(file)s'"

#: logviewer.py:320
#, python-format
msgid "Error: Can't open file '%s'\n"
