msgid "Previous"
msgstr "Précédent"

#: logviewer.py:484
msgid "Next"
msgstr "Suivant"

#: logviewer.py:498
msgid "Delete Log File"
msgstr "Supprimer le fichier log"

#: logviewer.py:604
msgid "Error"
msgstr "Erreur"

#: logviewer.py:605
#, python-format
